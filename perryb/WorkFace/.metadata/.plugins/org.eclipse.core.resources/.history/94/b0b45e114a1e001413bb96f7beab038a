package hello;

public class Fizzy_Buzzy {

	public static void main(String[] args) {

		int number = 0;

		while (number < 1000) {

			number = number + 1;

			if ((number % 3 == 0) && (number % 5 == 0)) {
				System.out.println("Fizz Buzz");
			} else if (number % 3 == 0) {
				System.out.println("Fizz");
			} else if (number % 5 == 0) {
				System.out.println("Buzz");
			} else {
				System.out.println(number);
			}
		}

	}
}
