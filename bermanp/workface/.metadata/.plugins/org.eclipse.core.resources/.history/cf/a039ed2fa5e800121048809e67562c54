
public class seasonchecker {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		
		
		String day = "SaturDay";
		if(day.equals("Sunday")  ||  day.equals("SaturDay")){
		System.out.println("its teh weekend");}
		
		else { 
			System.out.println("its not teh weekend yet");
		}
}
}
